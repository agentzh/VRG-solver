/* syntax sugar definitions for the VRG engine */

prefix:<\>   "line".
prefix:<#>   "plane".

infix:<//>   "parallel".
infix:<T>    "orthogonal".
infix:<A>    "oblique".

infix:<~//>  "unparallel".
infix:<~T>   "nonorthogonal".
infix:<~A>   "nonoblique".

infix:<on>   "on".
infix:<~on>  "not_on".

infix_circumfix:<[ ]>  "space-relation".
infix_circumfix:<< >>  "vector-relation".
